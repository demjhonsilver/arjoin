{
    "name": "dsilver/arjoin",
    "description": "A simple PHP string binder / URL-friendly slugs package. It can bypass the emojis, icons, or character symbols to convert them into an accessible string format.",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "DSilver"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dsilver\\Arjoin\\": "src/"
        }
    },
    "homepage": "https://github.com/dsilver/arjoin",
    "repository": {
        "type": "git",
        "url": "https://github.com/dsilver/arjoin.git"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "keywords": ["slugs", "string", "slug", "url", "characters", "binder", "php", "package", "laravel"]
}
